#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    FILE *fp_all = fopen("sayilar.txt", "w");
    FILE *fp0 = fopen("sayilar0.txt", "w");
    FILE *fp1 = fopen("sayilar1.txt", "w");
    int sayi[100];

    if (fp_all == NULL || fp0 == NULL || fp1 == NULL) {
        return 1;
    }

    srand(time(0));

    for (int i = 0; i < 100; i++) {
        sayi[i] = rand() % 101;
        fprintf(fp_all, "%d\n", sayi[i]);
    }

    for (int a = 0; a < 100; a++) {
        if ((sayi[a] >> 5) & 1) {
            sayi[a] &= ~(1 << 3);
            fprintf(fp1, "%d\n", sayi[a]);
        } else {
            sayi[a] |= (1 << 4);
            fprintf(fp0, "%d\n", sayi[a]);
        }
    }

    fclose(fp_all);
    fclose(fp0);
    fclose(fp1);

    return 0;
}
